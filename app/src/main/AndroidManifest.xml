<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.chantsdesperance">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/logo_chants"
        android:label="Chants D&apos;esperance"
        android:roundIcon="@mipmap/logo_chants_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ChantsDEsperance"
        tools:targetApi="31">
        <activity
            android:name=".ChantActivity"
            android:exported="false" />
        <activity
            android:name=".ListChantsActivity"
            android:exported="false" />
        <activity
            android:name=".HomeActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.MaterialComponents.DayNight.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>